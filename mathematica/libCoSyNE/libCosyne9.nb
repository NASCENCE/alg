Cell[BoxData[
 RowBox[{
  RowBox[{"evolutionStep", "[", 
   RowBox[{"pop_", ",", "fitFn_", ",", 
    RowBox[{"OptionsPattern", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"mutate", "\[Rule]", "0.5"}], ",", 
       RowBox[{"minimize", "\[Rule]", "False"}], ",", 
       RowBox[{"permuteAll", "\[Rule]", "False"}], ",", 
       RowBox[{"verbose", "\[Rule]", "False"}], ",", 
       RowBox[{"elite", "\[Rule]", "0"}]}], "}"}], "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"fp", "=", 
       RowBox[{"Sort", "[", "pop", "]"}]}], ",", "p2", ",", "fmin", ",", 
      "fmax", ",", "g2", ",", "t", ",", "r", ",", "res", ",", "perm", ",", 
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", 
        RowBox[{"Last", "@", 
         RowBox[{"First", "@", "pop"}]}], "]"}]}], ",", "fit", ",", "tbl", 
      ",", 
      RowBox[{"mprob", "=", 
       RowBox[{"OptionValue", "[", "mutate", "]"}]}], ",", "el", ",", 
      "newIndiv2", ",", "newFit2"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Reverse", " ", "if", " ", "maximizing"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\[Not]", 
        RowBox[{"OptionValue", "[", "minimize", "]"}]}], ",", 
       RowBox[{"fp", "=", 
        RowBox[{"Reverse", "@", "fp"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"store", " ", "elite"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"el", "=", 
      RowBox[{"fp", "\[LeftDoubleBracket]", 
       RowBox[{"1", ";;", 
        RowBox[{"OptionValue", "[", "elite", "]"}]}], 
       "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "take", " ", "the", " ", "top", " ", "quarter", " ", "and", " ", 
       "mutate", " ", "it", " ", "twice", " ", "to", " ", "get", " ", "one", 
       " ", "half", " ", "size", " ", "of", " ", "new", " ", "individuals"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"p2", "=", 
      RowBox[{"mapFn", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"prependFitness", "[", 
          RowBox[{"#", ",", "fitFn"}], "]"}], "&"}], ",", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"cauchyMutate", "[", 
            RowBox[{"#", ",", "mprob"}], "]"}], "&"}], "/@", 
          RowBox[{
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"fp", "\[LeftDoubleBracket]", 
              RowBox[{"1", ";;", 
               RowBox[{
                RowBox[{"Length", "[", "fp", "]"}], "/", "4"}]}], 
              "\[RightDoubleBracket]"}], ",", 
             RowBox[{"fp", "\[LeftDoubleBracket]", 
              RowBox[{"1", ";;", 
               RowBox[{
                RowBox[{"Length", "[", "fp", "]"}], "/", "4"}]}], 
              "\[RightDoubleBracket]"}]}], "]"}], "\[LeftDoubleBracket]", 
           RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}]}], ")"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "take", " ", "the", " ", "original", " ", "genomes", " ", "and", " ", 
       "their", " ", "fitness", " ", "separately"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"g2", "=", 
      RowBox[{"fp", "\[LeftDoubleBracket]", 
       RowBox[{"All", ",", "2"}], "\[RightDoubleBracket]"}]}], ";", 
     RowBox[{"fit", "=", 
      RowBox[{"First", "/@", "fp"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "compute", " ", "the", " ", "poermutations", " ", "probabilities"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"t", "=", 
      RowBox[{"ParallelTable", "[", 
       RowBox[{
        RowBox[{"Power", "[", 
         RowBox[{
          RowBox[{"Rescale", "[", 
           RowBox[{
            RowBox[{
            "fit", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
            ",", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "@@", 
               RowBox[{"(", 
                RowBox[{"First", "/@", "fp"}], ")"}]}], "&"}], "/@", 
             RowBox[{"{", 
              RowBox[{"Min", ",", "Max"}], "}"}]}]}], "]"}], ",", 
          RowBox[{"1", "/", "n"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "pop", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"for", " ", "maximizing"}], ",", " ", 
       RowBox[{"1", " ", "-", " ", "probabilities"}], ",", " ", 
       RowBox[{
       "such", " ", "that", " ", "bests", " ", "are", " ", "les", " ", 
        "permuted"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\[Not]", 
        RowBox[{"OptionValue", "[", "minimize", "]"}]}], ",", 
       RowBox[{"t", "=", 
        RowBox[{"1", "-", "t"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
