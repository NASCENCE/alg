          "]"}], ",", "p2"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"reverse", " ", "if", " ", "maximizing"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\[Not]", 
        RowBox[{"OptionValue", "[", "minimize", "]"}]}], ",", 
       RowBox[{"res", "=", 
        RowBox[{"Reverse", "@", "res"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"cut", " ", "off", " ", "the", " ", "worst"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
