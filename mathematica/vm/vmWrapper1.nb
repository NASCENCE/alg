Cell[BoxData[
 RowBox[{
  RowBox[{"vmPath", "=", "\"\<git/NASCENCE/vm\>\""}], ";"}]], "Input",
 CellChangeTimes->{{3.622223531427229*^9, 3.622223579385215*^9}, {
  3.6222239159614677`*^9, 3.622223916124701*^9}, {3.6222258486475687`*^9, 
  3.622225854120181*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
